		const toDay = s =>
			Math.ceil(
				new Date(s).getTime() - new Date(`${s.split("-")[0]}-01-01`).getTime()
			) / (1000 * 3600 * 24)

		suzuri.age = 22
		suzuri.birthYear = 2017 + 10000 - suzuri.age
